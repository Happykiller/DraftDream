{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "RWBucket",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:AbortMultipartUpload",
        "s3:ListBucket",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::draftdream-io",
        "arn:aws:s3:::draftdream-io/*"
      ]
    }
  ]
}
